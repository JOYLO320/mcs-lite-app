  "scripts": {
    "build": "NODE_ENV=production babel src --ignore src/__tests__/ --out-dir lib",
    "check:env": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "clean": "rm -r -f lib/*",
    "copy": "find ./src/ -iname '*.css' > _file.log; cat _file.log | while read from ; do to=$(echo $from | sed 's/src/lib/g'); mkdir -p `dirname $to` ; cp -avR $from $to; done; rm -rf ./_file.log;",
    "eslint": "node --max_old_space_size=4096 node_modules/.bin/eslint ./",
    "lint": "npm run stylelint && npm run eslint ",
    "postbuild": "npm run copy",
    "prebuild": "npm run clean",
    "stylelint": "babel-node node_modules/.bin/stylelint 'src/**/*.css' 'example/styles/**/*.css' 'example/scripts/**/*.css'",
    "test": "NODE_ENV=production karma start",
    "test:watch": "TESTTEST=true npm run test"
  },
